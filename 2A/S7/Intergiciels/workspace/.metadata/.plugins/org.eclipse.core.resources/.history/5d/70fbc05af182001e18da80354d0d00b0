import java.rmi.*;
import java.rmi.server.UnicastRemoteObject;

public class SFicheImpl implements SFiche {

	private String Nom;
	private String Email;
	
	SFicheImpl(String Nom, String Email) throws RemoteException {
		this.Nom = Nom;
		this.Email = Email;
	}

	@Override
	public String getNom() {
		return this.Nom;
	}

	@Override
	public String getEmail() {
		return this.Email;
	}
	
}