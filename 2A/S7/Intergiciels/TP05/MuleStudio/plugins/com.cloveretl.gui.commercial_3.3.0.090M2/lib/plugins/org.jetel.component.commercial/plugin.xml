<plugin
   id="com.opensys.clover.commercial"
   version="${version.full}"
   provider-name="Javlin a.s.">

	<runtime>
		<library path="cloveretl.component.commercial.jar"/>
		<library path="lib/mail-1.4.2.jar"/>
		<library path="lib/activation.jar"/>
		<library path="lib/axiom-api-1.2.8.jar"/>
		<library path="lib/axiom-dom-1.2.8.jar"/>
		<library path="lib/axiom-impl-1.2.8.jar"/>
		<library path="lib/axis2-adb-1.5.jar"/>
		<library path="lib/axis2-jaxws-1.5.jar"/>
		<library path="lib/axis2-kernel-1.5.jar"/>
		<library path="lib/axis2-transport-http-1.5.jar"/>
		<library path="lib/axis2-transport-local-1.5.jar"/>
		<library path="lib/commons-fileupload-1.2.1.jar"/>
		<library path="lib/commons-httpclient-3.1.jar"/>
		<library path="lib/commons-io-1.4.jar"/>
		<library path="lib/commons-jxpath-1.3.jar"/>
		<library path="lib/commons-beanutils-core-1.8.3.jar"/>
		<library path="lib/dnsjava-2.0.6.jar"/>
		<library path="lib/dom4j-1.6.1.jar"/>
		<library path="lib/httpcore-4.0.jar"/>
		<library path="lib/jsr181-api.jar"/>
		<library path="lib/neethi-2.0.4.jar"/>
		<library path="lib/stax-1.2.0.jar"/>
		<library path="lib/woden-api-1.0M8.jar"/>
		<library path="lib/wsdl4j-1.6.2.jar"/>
		<library path="lib/XmlSchema-1.4.7.jar"/>
		<library path="lib/jackson-core-asl-1.9.3.jar"/>
   	</runtime> 

	<requires engine-version="${version.full}">
		<import plugin-id="org.jetel.component"/>
		<import plugin-id="org.jetel.sequence"/>
	</requires>
	
	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.EmailFilter"/>
		<parameter id="type" value="EMAIL_FILTER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.EmailSender"/>
		<parameter id="type" value="EMAIL_SENDER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.EmailReader"/>
		<parameter id="type" value="EMAIL_READER"/>
	</extension>

	<extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.FastSort"/>
	    <parameter id="type" value="FAST_SORT"/>
	</extension>
	
	<extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.MetaPivot"/>
	    <parameter id="type" value="META_PIVOT"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.MultiLevelReader"/>
		<parameter id="type" value="MULTI_LEVEL_READER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.RelationalJoin"/>
		<parameter id="type" value="RELATIONAL_JOIN"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.ParallelReader"/>
		<parameter id="type" value="PARALLEL_READER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.ClusterPartitioner"/>
		<parameter id="type" value="CLUSTER_PARTITIONER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.ClusterGather"/>
		<parameter id="type" value="CLUSTER_GATHER"/>
	</extension>

	<extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.WebServiceClient"/>
	    <parameter id="type" value="WEB_SERVICE_CLIENT"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.complexdatareader.ComplexDataReader"/>
		<parameter id="type" value="COMPLEX_DATA_READER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.DataSampler"/>
		<parameter id="type" value="DATA_SAMPLER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.Pivot"/>
		<parameter id="type" value="PIVOT"/>
	</extension>

    <extension point-id="component">
        <parameter id="className" value="com.opensys.cloveretl.component.SpreadsheetReader"/>
        <parameter id="type" value="SPREADSHEET_READER"/>
    </extension>
      
    <extension point-id="component">
        <parameter id="className" value="com.opensys.cloveretl.component.SpreadsheetWriter"/>
        <parameter id="type" value="SPREADSHEET_WRITER"/>
    </extension>
    
	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.BeanReader"/>
		<parameter id="type" value="BEAN_READER"/>
	</extension>
      
    <extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.BeanWriter"/>
	    <parameter id="type" value="BEAN_WRITER"/>
	</extension>
	
	<extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.MapWriter"/>
	    <parameter id="type" value="MAP_WRITER"/>
	</extension>
	
	<extension point-id="component">
	    <parameter id="className" value="com.opensys.cloveretl.component.JsonReader"/>
	    <parameter id="type" value="JSON_READER"/>
	</extension>

	<extension point-id="component">
		<parameter id="className" value="com.opensys.cloveretl.component.JsonWriter"/>
		<parameter id="type" value="JSON_WRITER"/>
	</extension>

</plugin>
