<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.cloveretl.gui.component">
      <component
            file="components.xml"
            id="com.cloveretl.gui.commercial.component">
      </component>
   </extension>
   <extension
         point="com.cloveretl.gui.enginePlugin">
      <enginePlugin
            classLoaderProvider="com.cloveretl.gui.commercial.plugin.CLProvider"
            directory="lib/plugins"
            id="com.cloveretl.gui.commercial.enginePlugin">
         <exportedPlugin
               pluginId="org.jetel.component.commercial">
         </exportedPlugin>
         <exportedPlugin
               pluginId="org.jetel.lookup.commercial">
         </exportedPlugin>
         <exportedPlugin
               pluginId="org.jetel.compiler.commercial">
         </exportedPlugin>
         <exportedPlugin
               pluginId="org.jetel.ctlfunction.commercial">
         </exportedPlugin>
         <exportedPlugin
               pluginId="org.jetel.tlfunction.commercial">
         </exportedPlugin>
      </enginePlugin>
   </extension>
   <extension
         point="com.cloveretl.gui.editor">
      
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIEmailMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIEmailMappingPropertyToolkit"
            name="emailMapping">
      </editor>
      
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIWSFaultMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.WSFaultMappingEditor"
            name="wsFaultMapping">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIWSOperationNamePropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.WSOperationNameEditor"
            name="wsOperationName">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIWSRequestStructurePropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.WSRequestStructureEditor"
            name="wsRequestStructure">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIWSResponseMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.WSResponseMappingEditor"
            name="wsResponseMapping">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIWSServiceNamePropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.WSServiceNameEditor"
            name="wsServiceName">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="statefulTransform">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformMDPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformMDPropertyToolkit"
            name="statefulTransformMetadata">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformSPPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIStatefulTransformSPPropertyToolkit"
            name="statefulTransformSelectorProperties">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UITransformPivotPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.TransformPivotEditor"
            name="transformPivot">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UISpreadsheetSheetNamePropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UISpreadsheetSheetNamePropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringTypeDescriptorProvider"
            name="spreadsheetSheetName">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UISpreadsheetMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UISpreadsheetMappingPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="spreadsheetMapping">
      </editor>
      <editor
