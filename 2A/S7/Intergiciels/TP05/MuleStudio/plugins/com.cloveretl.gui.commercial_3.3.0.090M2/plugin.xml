            name="%newWizards.category.name"
            id="com.cloveretl.gui">
      </category>
      <wizard
            name="%DBWizard.name"
            icon="icons/connection-external-16.png"
            category="com.cloveretl.gui"
            class="com.cloveretl.gui.wizard.dbconnection.DBWizard"
            id="com.cloveretl.gui.wizard.dbConnection.DBWizard">
         <description>
            %DBWizard.description
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="%MetadataByHandWizard.name"
            icon="icons/metadata-external-16.png"
            category="com.cloveretl.gui"
