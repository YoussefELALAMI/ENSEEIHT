      		id="com.cloveretl.gui.commercial.propertytoolkit.UIJsonMappingPropertyToolkit"
      		typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
      		name="jsonReaderMapping">
      </editor>
      <editor
      		class="com.cloveretl.gui.commercial.propertytoolkit.UIJavaBeanMappingPropertyToolkit"
      		id="com.cloveretl.gui.commercial.propertytoolkit.UIJavaBeanMappingPropertyToolkit"
      		typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
      		name="javaBeanReaderMapping">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIObjectIntrospectorPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIObjectIntrospectorPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="objectIntrospector">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIBeanWriterMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIBeanWriterMappingPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="beanWriterMapping">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIJsonWriterMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIJsonWriterMappingPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="jsonWriterMapping">
      </editor>
      <editor
            class="com.cloveretl.gui.commercial.propertytoolkit.UIMapWriterMappingPropertyToolkit"
            id="com.cloveretl.gui.commercial.propertytoolkit.UIMapWriterMappingPropertyToolkit"
            typeDescriptorProviderClass="com.cloveretl.gui.model.descriptors.ExStringAsCDATATypeDescriptorProvider"
            name="mapWriterMapping"
      />
   </extension>
   <extension
         point="com.cloveretl.gui.lookupTable">
      <lookupTable
            class="com.cloveretl.gui.commercial.plugin.AspellLookupTablePlugin"
            id="com.cloveretl.gui.commercial.model.AspellLookupTable"
            name="%AspellLookupTable.name"
            type="aspellLookup">
      </lookupTable>
      <lookupTable
            class="com.cloveretl.gui.commercial.plugin.DBLookupTablePlugin"
            id="com.cloveretl.gui.commercial.model.DBLookupTable"
            name="%DBLookupTable.name"
            type="dbLookup">
      </lookupTable>
      <lookupTable
            class="com.cloveretl.gui.commercial.plugin.PersistentLookupTablePlugin"
            id="com.cloveretl.gui.commercial.model.PersistentLookupTable"
            name="%PersistentLookupTable.name"
            type="persistentLookup">
      </lookupTable>
      <lookupTable
            class="com.cloveretl.gui.commercial.plugin.RangeLookupTablePlugin"
            id="com.cloveretl.gui.commercial.model.RangeLookupTable"
            name="%RangeLookupTable.name"
            type="rangeLookup">
      </lookupTable>
   </extension>
   <extension
