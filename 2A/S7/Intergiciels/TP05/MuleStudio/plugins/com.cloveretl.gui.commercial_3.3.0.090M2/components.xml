  <ETLComponent category="readers" iconPath="icons/readers/SpreadsheetDataReader" name="SpreadsheetDataReader" type="SPREADSHEET_READER" viewPlainData="false">
    <shortDescription>Reads data from Excel files.</shortDescription>
    <description>Reads specified sheets of Excel files, converts their contents to data records and sends them to connected output port. Mapping of sheet content to output metadata must be defined.</description>
    <inputPorts>
      <singlePort name="0" required="false" label="input" />
    </inputPorts>
    <outputPorts>
      <singlePort name="0" required="true" label="output"/>
      <singlePort name="1" label="logs"/>
    </outputPorts>
    <properties>
      <property category="basic" displayName="File URL" modifiable="true" name="fileURL" nullable="true" required="true">
        <singleType name="file" fields="input" dictionary="input" selectionMode="file_only" />
      </property>
      <property category="basic" displayName="Sheet" modifiable="true" name="sheet" nullable="true" defaultHint="0">
        <singleType name="spreadsheetSheetName" multiselect="true"/>
      </property>
      <property category="basic" displayName="Mapping" modifiable="true" name="mapping" redundant="mappingURL">
        <singleType name="spreadsheetMapping"/>
      </property>
      <property category="basic" displayName="Mapping URL" modifiable="true" name="mappingURL" nullable="true" redundant="mapping">
        <singleType name="file"/>
      </property>
      <property category="basic" displayName="Data policy" modifiable="true" name="dataPolicy" nullable="true" defaultHint="Strict">
        <enumType>
          <item displayValue="Strict" value="strict" />
          <item displayValue="Controlled" value="controlled" />
          <item displayValue="Lenient" value="lenient" />
        </enumType>
      </property>
      <property category="advanced" displayName="Read mode" modifiable="true" name="attitude" nullable="true" defaultHint="Stream">
        <enumType>
          <item displayValue="In-memory" value="IN_MEMORY" />
          <item displayValue="Stream" value="STREAM" />
        </enumType>
      </property>
      <property category="advanced" displayName="Number of skipped records" modifiable="true" name="skipRecords" nullable="true">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Max number of records" modifiable="true" name="numRecords" nullable="true">
        <singleType name="int" min="1" />
      </property>
      <property category="advanced" displayName="Number of skipped records per source" modifiable="true" name="skipSourceRecords" nullable="true" defaultHintProvider="skipSourceRecords">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Max number of records per source" modifiable="true" name="numSourceRecords" nullable="true">
        <singleType name="int" min="1"/>
      </property>
      <property category="advanced" displayName="Number of skipped records per spreadsheet" modifiable="true" name="skipSheetRecords" nullable="true">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Max number of records per spreadsheet" modifiable="true" name="numSheetRecords" nullable="true">
        <singleType name="int" min="1"/>
      </property>
      <property category="advanced" displayName="Max error count" modifiable="true" name="maxErrorCount" nullable="true" defaultHint="(unlimited)">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Incremental file" modifiable="true" name="incrementalFile" nullable="true" required="incrementalKey">
        <singleType name="file" />
      </property>
      <property category="advanced" displayName="Incremental key" modifiable="true" name="incrementalKey" nullable="true" required="incrementalFile">
        <singleType name="string" />
      </property>
      <property category="advanced" displayName="Encryption password" modifiable="true" name="password" nullable="true" required="false">
            <singleType name="string"/>
      </property>
    </properties>
  </ETLComponent>
  
  <ETLComponent category="writers" iconPath="icons/writers/SpreadsheetDataWriter" name="SpreadsheetDataWriter" type="SPREADSHEET_WRITER" viewPlainData="false" viewGridData="false">
