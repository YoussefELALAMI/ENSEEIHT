    <description>Writes into specified sheets of Excel files. Mapping of input metadata to sheet content must be defined.</description>
    <inputPorts>
      <singlePort name="0" required="true" label="input" />
    </inputPorts>
    <outputPorts>
      <singlePort name="0" required="false" label="output"/>
    </outputPorts>
    <properties>
      <!-- basic properties -->
      <property category="basic" displayName="File URL" modifiable="true" name="fileURL" nullable="true" required="true">
        <singleType name="file" fields="output" dictionary="output" selectionMode="file_only" mandatory_existing_file="false"/>
      </property>
      <property category="basic" displayName="Sheet" modifiable="true" name="sheet" nullable="true" defaultHint="Create new sheet">
        <singleType name="spreadsheetSheetName" />
      </property>
      <property category="basic" displayName="Mapping" modifiable="true" name="mapping" redundant="mappingURL">
        <singleType name="spreadsheetMapping"/>
      </property>
      <property category="basic" displayName="Mapping URL" modifiable="true" name="mappingURL" nullable="true" redundant="mapping">
        <singleType name="file"/>
      </property>
      <property category="basic" displayName="Write mode" modifiable="true" name="writeMode" nullable="false" defaultValue="OVERWRITE_SHEET_IN_MEMORY">
        <enumType>
          <item displayValue="Overwrite in sheet (in-memory)" value="OVERWRITE_SHEET_IN_MEMORY"/>
              <item displayValue="Insert into sheet (in-memory)" value="INSERT_INTO_SHEET_IN_MEMORY"/>
              <item displayValue="Append to sheet (in-memory)" value="APPEND_TO_SHEET_IN_MEMORY"/>
              <item displayValue="Create new file (streaming - XLSX only)" value="CREATE_FILE_IN_STREAM"/>
              <item displayValue="Create new file (in-memory)" value="CREATE_FILE_IN_MEMORY"/>
        </enumType>
      </property>
      <property category="basic" displayName="Actions on existing sheets" modifiable="true" name="existingSheetsActions" nullable="false" defaultValue="DO_NOTHING">
        <enumType>
          <item displayValue="Do nothing, keep existing sheets and data" value="DO_NOTHING"/>
          <item displayValue="Clear target sheet(s)" value="CLEAR_SHEETS"/>
          <item displayValue="Replace all existing sheets" value="REMOVE_SHEETS"/> 
        </enumType>
      </property>      
      
      <!--advanced properties -->
      <property category="advanced" displayName="Template File URL" modifiable="true" name="templateFileURL" nullable="true" required="false">
        <singleType name="file" fields="output" dictionary="output" selectionMode="file_only" />
      </property>
      <property category="advanced" displayName="Create directories" modifiable="true" name="makeDirs" nullable="true" defaultHint="false">
        <singleType name="bool" />
      </property>
      <property category="advanced" displayName="Records per file" modifiable="true" name="numFileRecords" nullable="true">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Number of skipped records" modifiable="true" name="skipRecords" nullable="true">
        <singleType name="int" />
      </property>
      <property category="advanced" displayName="Max number of records" modifiable="true" name="numRecords" nullable="true">
        <singleType name="int" min="1" />
      </property>
      <property category="advanced" displayName="Partition key" modifiable="true" name="partitionKey" nullable="true" required="partition">
        <singleType name="key" inputPortName="0" />
      </property>
      <property category="advanced" displayName="Partition lookup table" modifiable="true" name="partition" nullable="true" required="partitionOutFields">
        <singleType name="lookupTableID" />
      </property>
      <property category="advanced" displayName="Partition file tag" modifiable="true" name="partitionFileTag" nullable="true" defaultHint="Number file tag">
        <enumType>
          <item displayValue="Number file tag" value="numberFileTag" />
          <item displayValue="Key file tag" value="keyNameFileTag" />
        </enumType>
      </property>
      <property category="advanced" displayName="Partition output fields" modifiable="true" name="partitionOutFields" nullable="true" required="partition">
        <singleType name="key" lookupMatadataProp="partition" />
      </property>
      <property category="advanced" displayName="Partition unassigned file name" modifiable="true" name="partitionUnassignedFileName" nullable="true" redundant="!partition">
        <singleType name="string" />
      </property>
      <property category="advanced" displayName="Type of formatter" modifiable="true" name="formatter" nullable="true" defaultHint="Auto">
        <enumType>
          <item displayValue="Auto" value="auto"/>
          <item displayValue="XLS" value="XLS"/>
          <item displayValue="XLSX" value="XLSX"/>
        </enumType>
      </property>
    </properties>
  </ETLComponent>
